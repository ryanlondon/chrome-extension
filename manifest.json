{
  "manifest_version": 2,

  "name": "Multi Highlighter",
  "description": "Highlights multiple words",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ],

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html",
   "default_title": "Multi Highlighter"
  },
  "permissions": [
    "tabs",
    "http://*/*", 
    "https://*/*"
  ]

  // "background": {
  //   "scripts": ["background.js"]
  // }
}


