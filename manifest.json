{
  "manifest_version": 2,

  "name": "Highlighter",
  "description": "Highlights multiple words",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],
   "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["background.js"]
    }
  ]
}